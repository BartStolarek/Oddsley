[run]
source = .
omit = 
    */tests/*
    */migrations/*
    */asgi.py
    */wsgi.py
    manage.py
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise NotImplementedError
    pass
    except ImportError:
    def setUp
    def tearDown

[html]
directory = htmlcov